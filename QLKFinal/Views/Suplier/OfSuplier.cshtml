@model System.Linq.IQueryable<QLKFinal.Models.Objectss>

@{
    ViewBag.Title = "Danh sách vật phẩm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Danh sách vật phẩm</h2>

<table id="table" class="table table-hover table-bordered">
    <thead>
    <tr>
        <th>Mã vật phẩm</th>
        <th>Tên vật phẩm</th>
        <th>Số lượng</th>
        <th>Đơn vị</th>
        <th>Ngày thêm</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var objects in Model)
    {
        <tr>
            <td>@objects.Id</td>
            <td>@objects.DisplayName</td>
            <td>@objects.Count</td>
            <td>@objects.Unit.DisplayName</td>
            <td>@objects.DateAdded.Value.ToShortDateString()</td>
        </tr>
    }
    </tbody>
</table>
@section scripts
{
    <script>$("#table").DataTable()</script>
}
